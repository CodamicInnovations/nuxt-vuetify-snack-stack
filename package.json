    "@nuxt/schema": "^3.2.0",
    "@nuxt/types": "^2.16.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "changelogen": "^0.4.1",
    "eslint": "^8.34.0",
    "h3": "~1.2.0",
    "nuxt": "^2.16.0",
    "sass": "^1.58.0",
    "vitest": "^0.28.4",
    "webpack": "^4.46.0"
  }
